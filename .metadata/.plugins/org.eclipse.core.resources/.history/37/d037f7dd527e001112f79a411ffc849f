package org.mt4j.input.inputProcessors.componentProcessors.xProcessor;
/* Erik Paluka - June 2011
 * Vialab at the University of Ontario Institute of Technology
 * Summer Research Assistant with Dr. Collins (Summer 2011)
 * This class holds the information needed by the X event.
 */
import org.mt4j.components.interfaces.IMTComponent3D;
import org.mt4j.input.inputProcessors.IInputProcessor;
import org.mt4j.input.inputProcessors.MTGestureEvent;

public class XEvent extends MTGestureEvent{


	/**
	 * Instantiates a new X event.
	 *
	 * @param source the source
	 * @param id the id
	 * @param targetComponent the target component
	 */
	public XEvent(IInputProcessor source, int id, IMTComponent3D targetComponent) {
		super(source, id, targetComponent);
	}

	

}
